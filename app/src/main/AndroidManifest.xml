    package="com.rahul.stressmanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/splashscreen">
        <activity android:name=".tabactivities.Home"></activity>
        <activity android:name=".tabactivities.chatbot" />
        <activity android:name=".tabactivities.List" />
        <activity android:name=".tabactivities.focous" />
        <activity android:name=".tabactivities.myDiet" />
        <activity android:name=".tabactivities.Favourite" />
        <activity android:name=".Activities.login" />
        <activity android:name=".Activities.Dashboard" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>